{
  "name": "test-nx-lerna",
  "version": "0.0.0",
  "license": "MIT",
  "private": false,
  "scripts": {
    "commit": "cz",
    "build": "nx affected --target=build",
    "lint": "nx affected --target=lint",
    "test": "nx affected --target=test",
    "e2e": "nx affected --target=e2e",
    "postversion": "nx affected --target=build",
    "publish": "lerna publish",
    "storybook": "nx --run-many --all --parallel=2 --target=storybook"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@digitalroute/cz-conventional-changelog-for-jira": "^7.1.0",
    "@nrwl/cli": "13.7.1",
    "@nrwl/cypress": "13.7.1",
    "@nrwl/eslint-plugin-nx": "13.7.1",
    "@nrwl/jest": "13.7.1",
    "@nrwl/linter": "13.7.1",
    "@nrwl/react": "^13.7.1",
    "@nrwl/storybook": "^13.7.1",
    "@nrwl/tao": "13.7.1",
    "@nrwl/web": "13.7.1",
    "@nrwl/workspace": "13.7.1",
    "@storybook/addon-essentials": "~6.4.12",
    "@storybook/builder-webpack5": "~6.4.12",
    "@storybook/manager-webpack5": "~6.4.12",
    "@storybook/react": "~6.4.12",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@types/react": "17.0.30",
    "@types/react-dom": "17.0.9",
    "@types/react-is": "17.0.2",
    "@types/styled-components": "5.1.15",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "babel-jest": "27.2.3",
    "babel-loader": "8.1.0",
    "babel-plugin-styled-components": "1.10.7",
    "commitizen": "^4.2.4",
    "cypress": "^9.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "~8.7.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.27.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "^7.0.4",
    "jest": "27.2.3",
    "lerna": "^4.0.0",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "react-test-renderer": "17.0.2",
    "ts-jest": "27.0.5",
    "typescript": "~4.5.2",
    "url-loader": "^3.0.0"
  },
  "dependencies": {
    "@testing-library/react": "^12.1.2",
    "core-js": "^3.6.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "17.0.2",
    "regenerator-runtime": "0.13.7",
    "styled-components": "5.3.3",
    "tslib": "^2.0.0"
  },
  "workspaces": [
    "packages/**"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm test"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json}": [
      "npm run lint:ts:fix"
    ]
  }
}
